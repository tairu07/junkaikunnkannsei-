{
  "version": 2,
  "functions": {
    "api/stocks.js": {
      "runtime": "nodejs18.x"
    },
    "api/favorites.js": {
      "runtime": "nodejs18.x"
    },
    "api/chart-settings.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
